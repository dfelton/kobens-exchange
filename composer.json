{
    "name": "kobens/kobens-exchange",
    "description": "Library for providing a common interface for interacting with Exchange accounts via API.",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Darren Felton",
            "email": "darren_felton@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/dfelton/kobens-core"
        },
        {
            "type": "git",
            "url": "https://github.com/dfelton/kobens-currency"
        }
    ],
    "require": {
        "php": "~7.4.0",
        "kobens/kobens-currency": "^3.0 || ^4.0",
        "kobens/kobens-core": "^3",
        "monolog/monolog": "^2.0",
        "symfony/console": "^4.2",
        "zendframework/zend-cache": "^2.8",
        "zendframework/zend-db": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Kobens\\Exchange\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KobensTest\\Exchange\\": "test"
        }
    }
}
